{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/admin",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/admin",
        "outputPath": "dist/apps/admin"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/admin"
        },
        "production": {}
      },
      "dependsOn": [
        "build-custom-server"
      ]
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "admin:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "admin:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "admin:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "admin:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/admin"
      ],
      "options": {
        "jestConfig": "apps/admin/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/admin/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build-custom-server": {
      "executor": "@nrwl/js:tsc",
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/admin",
        "main": "apps/admin/server/main.ts",
        "tsConfig": "apps/admin/tsconfig.server.json",
        "clean": false,
        "assets": []
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve-custom-server": {
      "executor": "@nrwl/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "admin:build-custom-server"
      },
      "configurations": {
        "development": {
          "buildTarget": "admin:build-custom-server:development"
        },
        "production": {
          "buildTarget": "admin:build-custom-server:production"
        }
      }
    }
  },
  "tags": []
}